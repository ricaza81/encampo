						<div class="photo-album-item-wrap col-4-width">
							{{> photo-album-items/photo-album-item4 }}
						</div>

						<div class="photo-album-item-wrap col-4-width">
							{{> photo-album-items/photo-album-item5 }}
						</div>

						<div class="photo-album-item-wrap col-4-width">
							{{> photo-album-items/photo-album-item6 }}
						</div>

						<div class="photo-album-item-wrap col-4-width">
							{{> photo-album-items/photo-album-item7 }}
						</div>

					</div>

				</div>
			</div>

		</div>
	</div>
</div>



