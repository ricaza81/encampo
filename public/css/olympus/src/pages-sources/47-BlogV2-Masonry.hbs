							{{> posts/postsV2-4}}

						</div>
					</div>
					<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item community">
						<div class="ui-block">

							{{> posts/postsV2-5}}

						</div>
					</div>
					<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item community">
						<div class="ui-block">

							{{> posts/postsV2-6}}

						</div>
					</div>
					<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item inspiration">
						<div class="ui-block">

							{{> posts/postsV2-7}}

						</div>
					</div>
					<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item community">
						<div class="ui-block">

							{{> posts/postsV2-8}}

						</div>
					</div>
					<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item inspiration">
						<div class="ui-block">

							{{> posts/postsV2-9}}

						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<a id="load-more-button" href="#" class="btn btn-control btn-more" data-load-link="posts-load-moreV2.html" data-container="posts-grid-1">
		<svg class="olymp-three-dots-icon"><use xlink:href="svg-icons/sprites/icons.svg#olymp-three-dots-icon"></use></svg>
	</a>
</div>




{{> windows-popup/window-chat-responsive}}

