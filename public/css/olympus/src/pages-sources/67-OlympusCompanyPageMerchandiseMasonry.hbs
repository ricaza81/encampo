<!DOCTYPE html>
<html lang="en">
<head>

	<title>Company Page - Merchandise Masonry</title>

	<!-- Required meta tags always come first -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta http-equiv="x-ua-compatible" content="ie=edge">

	<!-- Bootstrap CSS -->
	<link rel="stylesheet" type="text/css" href="Bootstrap/dist/css/bootstrap-reboot.css">
	<link rel="stylesheet" type="text/css" href="Bootstrap/dist/css/bootstrap.css">
	<link rel="stylesheet" type="text/css" href="Bootstrap/dist/css/bootstrap-grid.css">

	<!-- Main Styles CSS -->
	<link rel="stylesheet" type="text/css" href="css/main.min.css">
	<link rel="stylesheet" type="text/css" href="css/fonts.min.css">

	<!-- Main Font -->
	<script src="js/webfontloader.min.js"></script>
	<script>
		WebFont.load({
			google: {
				families: ['Roboto:300,400,500,700:latin']
			}
		});
	</script>

</head>
<body class="body-bg-white">


{{> stunning-headers/stunning-header-shop
	stunning-title = 'Merchandise'
}}




<section>
	<div class="container">
		<div class="row">

			<div class="col col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
				<div class="clients-grid">

					<ul class="cat-list-bg-style align-center sorting-menu cat-list__item-bg-blue">
						<li class="cat-list__item active" data-filter="*"><a href="#" class="">ALL PRODUCTS</a></li>
						<li class="cat-list__item" data-filter=".coffee-mugs"><a href="#" class="">COFFEE MUGS</a></li>
						<li class="cat-list__item" data-filter=".clothing"><a href="#" class="">CLOTHING</a></li>
						<li class="cat-list__item" data-filter=".accesories"><a href="#" class="">ACCESORIES</a></li>
					</ul>

					<div class="row sorting-container" id="products-grid-1" data-layout="masonry">
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item coffee-mugs">

							{{> shop/shop-product-item1-V2}}

						</div>
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item clothing">

							{{> shop/shop-product-item2-V2}}

						</div>
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item accesories">

							{{> shop/shop-product-item3-V2}}

						</div>
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item clothing">

							{{> shop/shop-product-item4-V2}}

						</div>
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item accesories">

							{{> shop/shop-product-item5-V2}}

						</div>
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item coffee-mugs">

							{{> shop/shop-product-item6-V2}}

						</div>
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item coffee-mugs">

							{{> shop/shop-product-item7-V2}}

						</div>
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item accesories">

							{{> shop/shop-product-item8-V2}}

						</div>
						<div class="col col-xl-4 col-lg-4 col-md-6 col-sm-12  sorting-item clothing">

							{{> shop/shop-product-item9-V2}}

						</div>
					</div>
				</div>
			</div>
		</div>

		<a id="load-more-button" href="#" class="btn btn-control btn-more" data-load-link="products-load-moreV2.html" data-container="products-grid-1">
			<svg class="olymp-three-dots-icon"><use xlink:href="svg-icons/sprites/icons.svg#olymp-three-dots-icon"></use></svg>
		</a>
	</div>
</section>


{{> animation-sections/call-to-action-animation}}

{{> windows-popup/registration-login-form-popup}}

{{> footers/footer-full-width}}

{{> windows-popup/window-chat-responsive}}

{{> back-to-top}}


